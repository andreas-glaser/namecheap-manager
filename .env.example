# .env.example

# Your Namecheap API username
NC_API_USER=your_api_user

# Your Namecheap API key
NC_API_KEY=your_api_key

# Your Namecheap account username
NC_USERNAME=your_username

# Your static IP address registered with Namecheap API
NC_CLIENT_IP=your_client_ip

# Optional: Custom endpoint (default is the official Namecheap endpoint)
# NC_ENDPOINT=https://api.namecheap.com/xml.response